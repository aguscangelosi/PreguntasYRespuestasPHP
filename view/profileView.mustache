<div class="row mt-3">
    <div class="col-1"></div>
    <div class="col">
        <div class="row g-0 bg-body-secondary position-relative rounded">
            <div class="col-md-6 mb-md-0 p-md-4">
                <img src="..." class="w-100" alt="...">
            </div>
            <div class="col-md-6 p-4 ps-md-0">
                <h3 class="p-2">Categorías con más respuestas correctas</h3>
                <div class="progress-stacked">
                    <div class="progress" role="progressbar" aria-label="Segment one" aria-valuenow="15" aria-valuemin="0" aria-valuemax="100" style="width: 15%">
                        <div class="progress-bar bg-success">Historia</div>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Segment two" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100" style="width: 30%">
                        <div class="progress-bar bg-danger">Anime</div>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Segment three" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%">
                        <div class="progress-bar bg-warning">Deportes</div>
                    </div>
                    <div class="progress" role="progressbar" aria-label="Segment four" aria-valuenow="35" aria-valuemin="0" aria-valuemax="100" style="width: 35%">
                        <div class="progress-bar bg-active">Ciencia</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-1"></div>
</div>
<div class="container">
    <div class="row m-3" style="height: 200px;">
        <div class="col m-1 bg-light text-dark text-center border border-secondary rounded border-2">
        {{#dataProfile}}
            <h1>ID</h1>
            <p class="fs-1 fw-bold m-auto">{{id}}</p>
        {{/dataProfile}}
        </div>
        {{#position}}
        <div class="col m-1 bg-light text-dark text-center border border-secondary rounded border-2">
            <h1>Mejor puntaje</h1>
            <p class="fs-1 fw-bold">{{puntaje}}</p>
        </div>
        <div class="col m-1 bg-light text-dark text-center border border-secondary rounded border-2">
            <h1>Ranking</h1>
            <p class="fs-1 fw-bold">{{posicion}}</p>
        </div>
        {{/position}}
        <div class="col m-1 bg-light text-dark text-center border border-secondary rounded border-2">
            <h1>QR</h1>
        </div>
    </div>
</div>


